'.text.html, .text.html.php':
  'Materialize badge with caption google icons':
    'prefix': 'm-badge:caption'
    'body': '<span class="badge" data-badge-caption="${1:custom caption}">${2:4}</span>'
  'Materialize new badge with caption and google icons':
    'prefix': 'm-badge:new-caption'
    'body': '<span class="new badge" data-badge-caption="${1:custom caption}">${2:4}</span>'
  'Materialize new badge with google icons':
    'prefix': 'm-badge'
    'body': '<span class="new badge">${1:Badge}</span>'
  'Materialize old badge':
    'prefix': 'm-badge-o'
    'body': '<span class="badge">${1:Badge}</span>'
  'Materialize breadcrumps':
    'prefix': 'm-breadcrumps'
    'body': """
      <nav>
        <div class="nav-wrapper">
          <div class="col s12">
            <a href="#!" class="breadcrumb">${1:First}</a>
            <a href="#!" class="breadcrumb">${2:Second}</a>
            <a href="#!" class="breadcrumb">${3:Third}</a>
          </div>
        </div>
      </nav>
      """
  'Materialize disabled':
    'prefix': 'm-button:button'
    'body': '<${1:button} ${2:type="button"} class="btn${3-large} disabled">${4:<i class="material-icons ${5:right}">${6:}"></i>}</${1:button}>'
